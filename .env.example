# Oscar Picture Book - Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration (for GLM-4-Flash)
OPENAI_API_KEY=your_openai_api_key_here

# CogView API Configuration (for image generation)
COGVIEW_API_KEY=your_cogview_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=production

# Vercel Configuration
# These will be set in Vercel dashboard
VERCEL_URL=your_vercel_app_url_here